  <model metaid="meta_6dad4416076e47acbc3d2726d2c79c52" sboTerm="SBO:0000293" id="ecoli_bounds" name="ecoli (BOUNDS)" substanceUnits="mmol" timeUnits="h" volumeUnits="l" areaUnits="m2" lengthUnits="m" extentUnits="mmol">
